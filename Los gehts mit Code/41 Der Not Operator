 for i in 1 ... 4 {
    moveForward()
    if isOnGem{collectGem()}
    turnLeft()
    if !isBlocked {
        move2()
        collectGem()
        turnLeft(); turnLeft()
        move2()
        turnLeft()} else {
            turnRight()}
}
func move2() {
    moveForward()
    jump()
 }
 
